{
  "name": "react-node-polyglot",
  "version": "0.2.1",
  "description": "Node polyglot react component",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "preview": "babel preview/src --out-dir preview/lib --copy-files",
    "start": "npm run build -- --watch & npm run preview -- --watch & gulp & node ./preview/server.js",
    "test": "npm run build && mocha \"test/**/*.js\"",
    "lint": "eslint --ext .es,.js --ignore-path .eslintignore --format codeframe ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrnamentStudio/react-node-polyglot.git"
  },
  "keywords": [
    "react",
    "node-polyglot"
  ],
  "author": "Abylay Keldibek <atomio.ak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OrnamentStudio/react-node-polyglot/issues"
  },
  "homepage": "https://github.com/OrnamentStudio/react-node-polyglot#readme",
  "engines": {
    "node": "^8.x.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "browserify": "^16.2.3",
    "connect-livereload": "^0.6.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "gulp": "^4.0.0",
    "livereload": "0.7.0",
    "node-polyglot": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "peerDependencies": {
    "react": "^16.x.x",
    "prop-types": "^15.x.x",
    "node-polyglot": "^2.x.x"
  },
  "dependencies": {
    "chai": "^4.2.0"
  }
}
